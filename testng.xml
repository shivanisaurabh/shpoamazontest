<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="methods" thread-count="2" verbose="1">

<listeners>
        <listener class-name="report.Screenshot" />
		
	</listeners>
<listeners>
      
		<listener class-name="report.ExtentReporterNG" />
	</listeners>

  <test name="Test">
    <classes>
      <class name="test.HomeTest"/>
        <class name="test.ProductTest"/>
          <class name="test.CartTest"/>
      <class name="test.CheckoutTest"/>
    </classes>
  </test> <!-- Test -->
  
  <test name="Mobile">
    <classes>
      <class name="test.HomeTest"/>
        <class name="test.ProductTest"/>
          <class name="test.CartTest"/>
      <class name="test.CheckoutTest"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
